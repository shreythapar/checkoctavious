@model IPagedList<Incedo_Octavius_Demo_2.Models.BusinessUserDegreeModel>

@{
    ViewBag.Title = "Index";
}
@using PagedList.Mvc;
@using PagedList;


@{
    Html.RenderPartial("_MapBU");
}

<h2>Degree Business Rules</h2>
<br/>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.First().Degree_Map)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().Degree_Master)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
